<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="useGeneratedKeys" value="true"/>
        <setting name="logImpl" value="SLF4J"/>
        <setting name="lazyLoadingEnabled" value="false"/>
        <setting name="cacheEnabled" value="false"/>
    </settings>
    <typeAliases>
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageUserDO" alias="HokageUserDO"/>
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageSupervisorServerDO" alias="HokageSupervisorServerDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageSubordinateServerDO" alias="HokageSubordinateServerDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageSupervisorSubordinateDO" alias="HokageSupervisorSubordinateDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageSecurityGroupDO" alias="HokageSecurityGroupDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageServerDO" alias="HokageServerDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageServerGroupDO" alias="HokageServerGroupDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageTaskDO" alias="HokageTaskDO" />
        <typeAlias type="com.banzhe.hokage.persistence.dataobject.HokageTaskResultDO" alias="HokageTaskResultDO" />
    </typeAliases>
    <mappers>
        <mapper resource="mybatis/mapper/hokage_user-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_supervisor_server-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_subordinate_server-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_supervisor_subordinate-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_security_group-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_server-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_server_group-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_taskr-mapper.xml"/>
        <mapper resource="mybatis/mapper/hokage_task_result-mapper.xml"/>
    </mappers>
</configuration>